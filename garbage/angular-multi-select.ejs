
<% title = "Angular multi select" %>
<% dependsOn( ["jquery", "bootstrap", "angular"]); %>

<script type="text/javascript" src="//cdn.rawgit.com/jagdeep-singh/angularMultipleSelect/e3413396/build/multiple-select.min.js"></script>
<link href="//cdn.rawgit.com/jagdeep-singh/angularMultipleSelect/e3413396/build/multiple-select.min.css" rel="stylesheet">

<script type="text/javascript" src="//cdn.rawgit.com/amitava82/angular-multiselect/cec79cde/dist/multiselect.js"></script>
<script type="text/javascript" src="//cdn.rawgit.com/amitava82/angular-multiselect/cec79cde/dist/multiselect-tpls.js"></script>
<link href="//cdn.rawgit.com/amitava82/angular-multiselect/cec79cde/dist/multiselect.css" rel="stylesheet">

<h1><%=title%></h1>

<div ng-app="myApp">
    <div ng-controller="myController">
        <multiple-autocomplete ng-model="selectedJobs"
                               object-property="name"
                               suggestions-arr="jobs">
        </multiple-autocomplete>
        <am-multiselect class="input-lg" multiple="true" ms-selected ="There are {{selectedCar.length}} car(s) selected"
                        ng-model="selectedCar" ms-header="Select Some Cars"
                        options="c.name for c in cars"></am-multiselect>
    </div>
</div>

<script>
    var app = angular.module('myApp', ['multipleSelect','am.multiselect']);
    app.controller('myController', function($scope) {
        $scope.jobs = [
            {id: 1,  name : "Java"},
            {id: 2,  name : "C"},
            {id: 3,  name : "C++"},
            {id: 4,  name : "AngularJs"},
            {id: 5,  name : "JavaScript"}
        ];
        $scope.cars = [
            {id:1, name: 'Audi'},
            {id:2, name: 'BMW'},
            {id:3, name: 'Honda'}
        ];
        $scope.selectedCar = [];
        
        $scope.$watch("selectedCar", function(cars){
            console.log(cars);
        })
    });
    
</script>