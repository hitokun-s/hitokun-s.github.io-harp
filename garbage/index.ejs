<% title = "Garbages" %>

<h1><i class="trash icon"></i>Garbages</h1>

<div class="row">
    <div class="ui link cards">
        <% for(var article of public.garbage._data.articles){ %>
        <a href="<%= article.href %>" class="card ui raised">
            <div class="content">
                <div class="header"><%= article.title %></div>
            </div>
        </a>
        <% } %>
    </div>
</div>

</div>
